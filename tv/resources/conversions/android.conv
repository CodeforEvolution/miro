[DEFAULT]
name: Android Devices
executable: ffmpeg
extension: m4v
parameters: -i {input} -vcodec libx264 -vpre slow -vpre ipod640 -sameq -s {ssize} -acodec aac -ab 160000 -r 18 {output}
mediatype: video

[G2]
ssize: 800x480

[Nexus One]
ssize: 800x400

[Dream / G1]
ssize: 480x320

[Magic / myTouch]
ssize: 480x320

[Droid]
ssize: 854x480

[Eris / Desire]
ssize: 480x320

[Hero]
ssize: 480x320

[Cliq / DEXT]
ssize: 480x320

[Behold II]
ssize: 480x320
