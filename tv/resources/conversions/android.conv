[DEFAULT]
name: Android Devices
executable: ffmpeg
extension: mp4
parameters: -i {input} -f mp4 -vcodec mpeg4 -sameq -s {ssize} -acodec aac -ab 160000 -r 18 {output}

[Nexus One]
ssize: 800x400

[Dream / G1]
ssize: 480x320

[Magic / myTouch]
ssize: 480x320

[Droid]
ssize: 854x480

[Eris / Desire]
ssize: 480x320

[Hero]
ssize: 480x320

[Cliq / DEXT]
ssize: 480x320

[Behold II]
ssize: 480x320
