		<pubDate>Wed, 22 Oct 2008 21:59:27 -0400</pubDate>
		<media:group>
			<media:content type="video/x-flv" expression="full" medium="video" duration="01:23">
				<media:thumbnail url="http://imagescdn.reuters.com/20081023/ftpo1218216651189.jpg" time="00:00:00.000" />
